<Application x:Class="STS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:STS"
             StartupUri="Windows/AuthWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="Profile" UriSource="/Images/profileIcon.png"/>
            <BitmapImage x:Key="FullStar" UriSource="/Images/fullStarImage.png"/>
            <BitmapImage x:Key="Bookmark" UriSource="/Images/bookMarkImage.png"/>
            <BitmapImage x:Key="Comment" UriSource="/Images/commentIcon.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
